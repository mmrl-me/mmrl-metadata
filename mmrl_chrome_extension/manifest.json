{
  "manifest_version": 3,
  "name": "mmrl",
  "version": "1.3",
  "description": "Inject Google forms with multiselect options for tagging",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  "content_scripts": [
    {
      "js": [
        "scripts/jquery-3.6.3.slim.min.js",
        "scripts/content.js",
        "scripts/multiselect-dropdown.js"
      ],
      "matches": [
        "https://docs.google.com/forms/d/e/1FAIpQLScgnrTkFOKuHLTVT3P-YwjbutoxB15HeLRQGbZGVGl5I-p1Ew/*",
        "https://docs.google.com/forms/d/e/1FAIpQLSc0CJSdBPbgfjMG78-ZlOd26oz2pmyhgvjtyaD1HT1-fEOpCQ/*"
      ]
    }
  ],
 "web_accessible_resources": [
    {      
      "matches": [ "https://docs.google.com/*" ],
      "resources": [ "data/*.json" ]
    }
  ]
}
